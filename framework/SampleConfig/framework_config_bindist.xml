<?xml version="1.0" encoding="utf-8"?>
<!-- Used in binary distribution of framework  - Contains only the settings that are relevant for it -->
<!--***Developers, when editing this file please update framework_config.xml and framework_config_win32dev.xml also***-->
<TSK_FRAMEWORK_CONFIG>
  <!-- Directory where configuration files and data can be found -->
  <CONFIG_DIR>.\</CONFIG_DIR>
  <!-- Directory where plug-in and executable modules can be found -->
  <MODULE_DIR>..\modules\</MODULE_DIR>  
  <!-- Directory where program using the framework is installed -->
  <!-- <PROG_DIR>X</PROG_DIR> -->
  <!-- Location of pipeline configuration file -->
  <PIPELINE_CONFIG_FILE>pipeline_config.xml</PIPELINE_CONFIG_FILE>

  <!-- Directory where unallocated sectors image files will be stored pending carving. The default directory is #OUT_DIR#\Carving.-->
  <!-- <CARVE_DIR>#OUT_DIR#\Carving</CARVE_DIR> -->
  <!-- File name to be given to all unallocated sectors image files. The default name is unalloc.bin-->
  <!-- <UNALLOC_SECTORS_IMG_FILE_NAME>unalloc.bin</UNALLOC_SECTORS_IMG_FILE_NAME> -->
  <!-- Maximum allowable size for unallocated sectors image files. Use 0 to have no maximum size and instead break files based on volume boundaries only. The default value is 0.-->
  <!-- <MAX_UNALLOC_SECTORS_IMG_FILE_SIZE>0</MAX_UNALLOC_SECTORS_IMG_FILE_SIZE> -->
  <!-- If true, unallocated sectors image files are not deleted after carving. The default value is false -->
  <!-- <CARVE_EXTRACT_KEEP_INPUT_FILES>true</CARVE_EXTRACT_KEEP_INPUT_FILES> -->
  <!-- If true, carved files are not deleted from the carving output directory after they are copied to file storage. The default value is false -->
  <!-- <CARVE_EXTRACT_KEEP_OUTPUT_FILES>true</CARVE_EXTRACT_KEEP_OUTPUT_FILES> -->

  <!-- Directory where Scalpel is installed -->
  <!-- <SCALPEL_DIR>X</SCALPEL_DIR> -->
  <!-- Scalpel configuration file. The default file is scalpel.conf in SCALPEL_DIR --> 
  <!-- <SCALPEL_CONFIG_FILE>X</SCALPEL_CONFIG_FILE> -->  
</TSK_FRAMEWORK_CONFIG>
